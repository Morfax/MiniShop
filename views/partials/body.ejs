<% switch (pagina) {
    case 'index' : %>
            <h1> <%= title %> </h1>
            <h1> <%= conten %> </h1>
            <% break;
    
    case 'productos' : %>
            <div class="col-2">
                <h1> Productos </h1>
                <% include rec/productos %>
            </div>
            <% break;
    
    default: %>
        <h1>Error</h1>
<% } %>